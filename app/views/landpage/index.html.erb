<!--blog posts section-->
<div class="home-blog text-center clearfix" id="landing-news">
    <div class="container">
        <div class="slogan-section animated fadeInUp clearfix">
            <h2>Latest News from <span>Health and Medical</span></h2>
            <p>There are several amazing news which advanced by our team members.</p>
        </div>
        <div class="row">
            <%  start_i = @blog_data.length - 1
                end_i   = @blog_data.length - 3
                click_i = 0
                if (@blog_data.length == 2)
                    start_i = 0
                    end_i = 1
                end
                if (@blog_data.length == 1)
                    start_i = 0
                    end_i = 0
                end
                for i in end_i..start_i 
                    if (@blog_data.length == 0)
                        break
                    end %>
                    <div class="col-md-4 news_read">
                        <article class="common-blog-post animated fadeInRight clearfix">
                            <div class="gallery gallery-slider clearfix loading">
                                <ul class="slides">
                                    <% if(File.exist?("savedimages/" + @blog_data[i].image) && @blog_data[i].image != '') %>
                                        <li>
                                            <a href="" title="" >
                                                <%  file = File.open("savedimages/" + @blog_data[i].image, "rb")
                                                    contents = file.read %>
                                                <img src="<%= contents %>" alt="gallery-1" />
                                            </a>
                                        </li>
                                    <% end %>
                                    <% if(File.exist?("savedimages/" + @blog_data[i].image1) && @blog_data[i].image1 != '') %>
                                        <li class="flex-active-slide">
                                            <a href="" title="" >
                                                <%  file = File.open("savedimages/" + @blog_data[i].image1, "rb")
                                                    contents = file.read %>
                                                <img src="<%= contents %>" alt="gallery-2" />
                                            </a>
                                        </li>
                                    <% end %>
                                    <% if(File.exist?("savedimages/" + @blog_data[i].image2) && @blog_data[i].image2 != '') %>
                                        <li>
                                            <a href="" title="" >
                                                <%  file = File.open("savedimages/" + @blog_data[i].image2, "rb")
                                                    contents = file.read %>
                                                <img src="<%= contents %>" alt="gallery-3" />
                                            </a>
                                        </li>
                                    <% end %>
                                </ul>
                            </div>
                            <div class="text-content clearfix">
                                <div class="entry-meta">
                                    <span><%= @blog_data[i].updated_at %></span>, by 
                                    <a href="#" title="Posts by John Doe">
                                        <%  if(Authcon.exists?(id: @blog_data[i].account_id))
                                                blog_owner = Authcon.find(@blog_data[i].account_id) %>
                                                <%= blog_owner.email %>
                                        <%  else %>
                                            <%= "Unkown" %>
                                        <%  end %>
                                    </a>
                                </div>
                                <div class="for-border"></div>
                                <p style="display: none;margin-top: 15px;font-size: 18px;font-weight: 600;"><%= @blog_data[i].title %></p>
                                <p><%= @blog_data[i].description %></p>
                                <p style="display: none;border-top: 1px solid lightgray"><%= @blog_data[i].detail %></p>
                            </div>
                        </article>
                        <a class="read-more" onclick="news_read_more(<%= click_i %>)">Read More</a>
                        <% click_i = click_i + 1 %>
                    </div>
            <%  end %>
        </div>
    </div>
</div>
