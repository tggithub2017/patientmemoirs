        end %>
        <div class="row">
            <% if AdminSpecialist.count == 3
                start_ri = 0
                end_ri = 2
            end %>
            <% if AdminSpecialist.count == 2
                start_ri = 0
                end_ri = 1
            end %>
            <% if AdminSpecialist.count == 1
                start_ri = 0
                end_ri = 0
            end %>
