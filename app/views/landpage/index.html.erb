<script>
    setTimeout(function() {
        $(window).on('load', function(){
            $('.common-blog-post .slides').find('img').each(function(){
                var imgClass = (this.width/this.height > 1) ? 'wide' : 'tall';
                var left = 360 - $(this).width();
                $(this).css('position', 'relative');
                $(this).addClass(imgClass);
            });

            $('.common-doctor figure').find('img').each(function(){
                var imgClass = (this.width/this.height > 1) ? 'wide1' : 'tall1';
                var left = 262 - $(this).width();
                $(this).css('position', 'relative');
                $(this).addClass(imgClass);
            });
        });
    }, 0);

    function news_read_more(index) {
        var length = $(".news_read:eq("+index+") .slides img").length;
        var count = 0;
        $(".news_read:eq("+index+") .slides img").each(function() {
            if(length == 5) {
                if(count < 3) {
                    $(".news_read_inner").append($(this).parent().html());
                }
            }
            if(length == 4) {
                if(count < 2) {
                    $(".news_read_inner").append($(this).parent().html());
                }             
            }
            if(length == 3) {
                if(count < 1) {
                    $(".news_read_inner").append($(this).parent().html());
                }             
            }
            count++;
        });
        $(".news_read_inner").append($(".news_read:eq("+index+") .text-content").html());
        $(".news_read_inner p").css("display", "block");

        $(".news_read_inner img").removeClass('wide');
        $(".news_read_inner img").removeClass('tall');
        $(".news_read_cover").css("display", "block");
        $(".news_read_inner").css("display", "grid");
        $("body").css("position", "fixed");
        $(".news_read_p").css("display", "block");
    }

    function news_read_morer(index) {
        var length = $(".specialists_read:eq("+index+") figure img").length;
        var count = 0;
        $(".specialists_read:eq("+index+") figure img").each(function() {
            $(".news_read_inner").append($(this).parent().html());
            count++;
        });
        $(".news_read_inner").append($(".specialists_read:eq("+index+") .text-content").html());
        $(".news_read_inner p").css("display", "block");
        $(".news_read_inner p:eq(0)").css("font-weight", "700");

        $(".news_read_inner img").removeClass('wide1');
        $(".news_read_inner img").removeClass('tall1');
        $(".news_read_cover").css("display", "block");
        $(".news_read_inner").css("display", "grid");
        $("body").css("position", "fixed");
        $(".news_read_p").css("display", "block");
    }

    function news_read_close() {
        $(".news_read_cover").css("display", "none");
        $(".news_read_inner").css("display", "none");
        $("body").css("position", "relative");
        $(".news_read_p").css("display", "none");
        $(".news_read_inner").html('');
    }
</script>

