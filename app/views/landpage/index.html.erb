                                <a>
                                    <%  file = File.open("savedimages/" + tmpSpecialists[i].image, "rb")
                                        contents = file.read %>
                                    <img src="<%= contents %>" alt="gallery-1" />
                                </a>
                            <% end %>
                        </figure>
                        <div class="text-content">
                            <h5><a href=""> <%= tmpSpecialists[i].title %> </a></h5>
                            <div class="for-border"></div>
                            <p>
                                <%= tmpSpecialists[i].description %>
